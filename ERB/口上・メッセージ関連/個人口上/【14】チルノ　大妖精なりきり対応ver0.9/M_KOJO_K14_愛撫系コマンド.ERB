@M_KOJO_MESSAGE_COM_K14_0_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM) && !FLAG:70
		IF TALENT:恋慕
			PRINTFORMDL	和琪露诺娇小柔软的身体贴在了一起、一边彼此对望着、一边把手搭在了妖精的腰上。
			PRINTFORMDL	从柔软的腋下、光滑的腰侧、一路抚摸到富有弹性的小巧臀部。
			PRINTFORML	「嗯、这样的感觉……咱不讨厌哦？再、再多摸一会也可以哦？」
			PRINTFORMDW	琪露诺的喘息声渐渐变重、脸色潮红地靠了过来。
		ELSEIF MARK:反発刻印 >= 3 || TCVAR:TARGET:强行
			PRINTFORMDL	把琪露诺娇小柔软的身体按到怀里、强行爱抚起了不太安定的妖精。
			PRINTFORMDL	从柔软的腋下、光滑的腰侧、一路抚摸到富有弹性的小巧臀部。
			PRINTFORML	「呀、讨厌、快住手啊！别碰咱！呜……」
			PRINTFORMDW	琪露诺的喘息声渐渐变重、脸上满是困惑和不解。
		ELSE
			PRINTFORMDL	和琪露诺娇小柔软的身体贴在了一起、一边彼此对望着、一边把手搭在了妖精的腰上。
			PRINTFORMDL	从柔软的腋下、光滑的腰侧、一路抚摸到富有弹性的小巧臀部。
			PRINTFORML	「嗯、这样的感觉……咱不讨厌哦？再、再多摸一会也可以哦？」
			PRINTFORMDW	琪露诺的喘息声渐渐变重、脸色潮红地靠了过来。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;挿入中
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	;V挿入
	IF LOCAL:1 && TEQUIP:50 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORML	「咿、好快、呜啊、而且还要、这样碰的话……」
			PRINTFORMDW	在即将射精的时候、用手掌配合着抚摸起了琪露诺的小腹。
			RETURN 1
		ELSE
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML	「啊……呜……感觉好奇怪……」
					PRINTFORMDW	一边往穴内抽送着、一边揉捏起了琪露诺小小的屁股。
				CASE 1
					PRINTFORML	「嗯啊、这样摸着、感觉麻麻的……」
					PRINTFORMDW	一边刺激着琪露诺的皮肤、一边突入到了膣内的深处。
			ENDSELECT
			RETURN 1
		ENDIF
	;A挿入
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;ふたなり
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSEIF TALENT:恋慕
		IF TALENT:無知
			IF (ABL:Ｃ感覚 + ABL:Ｂ感覚) >= 10
				PRINTFORMW	「唔、这样全都碰一遍的话……」
			ELSE
				PRINTFORMW	「在摸什么啊、好痒的啊！」
			ENDIF
		ELSE
			IF (ABL:Ｃ感覚 + ABL:Ｂ感覚) >= 10
				PRINTFORMW	「呜……手法真下流……」
			ELSE
				PRINTFORMW	「唔、都在摸什么地方啦……」
			ENDIF
		ENDIF
		RETURN 1
	ELSEIF TALENT:愛欲
		IF (ABL:Ｃ感覚 + ABL:Ｂ感覚) >= 10
			PRINTFORMW	「手法真下流……嗯、再多摸一会……」
		ELSE
			PRINTFORMW	「哼哼……真是好色的家伙……」
		ENDIF
		RETURN 1
	ELSE
		IF TALENT:無知
			IF ABL:欲望 >= 5
				PRINTFORMW	「嗯、明明不想被你这样碰的……」
			ELSE
				PRINTFORMW	「要是摸奇怪的地方咱就咬你哦！」
			ENDIF
		ELSE
			IF ABL:欲望 >= 5
				PRINTFORMW	「嗯、明明不想被你这样摸的……」
			ELSE
				PRINTFORMW	「别乱摸啊！……唔……」
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1
