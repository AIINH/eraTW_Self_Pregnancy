LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF FLAG:70
			RETURN 1
		ELSE
			;恋慕時のみ可能
			PRINTFORML 「唉、想要我穿上这个吗…？」
			PRINTFORML 「这、这种感觉就可以了吗、适合我吗？」
			PRINTW 
			PRINTFORML 「真的？　呼呼、谢谢你、亲・爱・的%UNICODE(0x2665) *1%」
			PRINTFORML 「Kyaー、说出来啦ー%UNICODE(0x2665) *1%」
			PRINTFORML 极不要不要得摇着头
			PRINTW 
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	;開始
	IF TEQUIP:新妻PLAY
		IF CFLAG:K42C_結婚 >= 1
			IF TEQUIP:0 == 0 && TEQUIP:1 == 0
				SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「欸嘿嘿…感觉怎样呢、亲爱的%UNICODE(0x2665) *1%」
					PRINTFORM 从围裙里偷看
					IF TALENT:TARGET:胸圍 > 0
						PRINTFORML 十分丰满圆润的胸部、视线钉在上面无法挪開…
					ELSEIF TALENT:TARGET:胸圍 == -1
						PRINTFORML 可爱的小巧的胸脯忽隐忽现的让人心跳不已…
					ELSEIF TALENT:TARGET:胸圍 == -2
						PRINTFORML 虽然是令人感到悲伤的绝壁、乳头忽隐忽现的很性感
					ELSE
						PRINTFORML 形状美妙的胸部、视线钉在上面无法挪開…
					ENDIF
					PRINTW 
					PRINTFORML 「啊ー、尽是盯着胸部ー。真是H…%UNICODE(0x2665) *1%」
					PRINTFORML 极假装生气的样子戳了戳你的额头
					PRINTW 
				CASE 1
					PRINTFORML 「穿这样子的你会很開心对吧…%UNICODE(0x2665) *1%」
					PRINTFORML 穿着裸体围裙的极、翘起屁股故意向这边展示着…
					PRINTW 
				CASE 2
					PRINTFORML 「穿、穿成这打扮就可以了吗…？」
					PRINTFORML 极扭扭捏捏的感到很害羞
					PRINTW 
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「亲爱的、欢迎回来%UNICODE(0x2665) *1%」
					PRINTFORML 「要吃饭吗？　要洗澡吗？　还是说、要・吃・我呢？」
					PRINTW 
					PRINTFORML 「虽然由自己说有点那个、穿着ー还是挺羞耻的呢、这个…」
					PRINTFORML 「呀嗯、不可以一边脱衣服一边扑过来呀%UNICODE(0x2665) *1%」
					PRINTW 
				CASE 1
					PRINTFORML 「呐ー呐ー适合我吗ー？」
					CALL ASK_YN("十分的适合","极酱是最可爱的新娘")
					IF !RESULT
						PRINTFORML 「真的？　好開心ー%UNICODE(0x2665) *1%」
						PRINTFORML 极带着灿烂的笑容抱住了你
						PRINTW 
					ELSE
						PRINTFORML 「真、真是的～～、称赞得太过了啦…\/\/\/」
						PRINTFORML 极是不是觉得很害羞呢、ほにゃほにゃと笑着いる
						PRINTW 
					ENDIF
				CASE 2
					PRINTFORML 「欸嘿嘿、已经不是玩新婚游戏、而是已经好好地结婚了呢…♪」
					PRINTFORML 「我很幸福哟、亲・爱・的%UNICODE(0x2665) *1%」
					PRINTW 
				ENDSELECT
			ENDIF
			RETURN 1
		ELSE
			IF TEQUIP:0 == 0 && TEQUIP:1 == 0
				SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「冷静地思考的话、穿着裸体围裙做饭会很危险」
					PRINTFORML 「因为我是妖怪所以热油溅起来的话不会受重伤的呢ー」
					PRINTW 
				CASE 1
					PRINTFORML 「裸体只穿着围裙…总觉得很羞耻呢ね」
					PRINTFORML 「但是、%MASTERNAME:42%、你看起来ー很開心呢ー…♪」
					PRINTW 
				CASE 2
					PRINTFORML 「啊、我、有在书里看过这样的」
					PRINTFORML 「穿着裸体围裙做着料理的妻子、被丈夫从后面抱住…H、要做的对吧…%UNICODE(0x2665) *1%」
					PRINTW 
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「欸嘿嘿、穿着这个感觉我真的成为了新妻呢…%UNICODE(0x2665) *1%」
					PRINTFORML 「唉、唉ー哆、呐、不要笑呢…亲・爱・的」
					PRINTFORML 「…………Kyaーー、真是的ー、好羞耻ー！　亲爱的什么的ー！讨厌ー\/\/\/」
					PRINTFORML 看着一个人演戏后一个人害羞起来的极真的不会看腻
					PRINTW 
				CASE 1
					PRINTFORML 「欸嘿嘿、总觉得、这样的很棒呢…%UNICODE(0x2665) *1%」
					PRINTFORML 极将手放到了围裙上、脸上露出感慨颇深的表情
					PRINTW 
				ENDSELECT
				RETURN 1
			ENDIF
		ENDIF
		RETURN 1
	;終了
	ELSE
		PRINTFORML 「啊ー嗯、再船上这个一会儿吧ー」
		PRINTW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1
