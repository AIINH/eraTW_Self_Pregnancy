;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		IF FLAG:70
			RETURN 1
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	ELSEIF TALENT:愛欲 || TALENT:炮友
		;強制舐陰←→舐陰は六九式対象外らしい
		;強制舐陰←→给对方口交も×
		;给对方口交←→口交はＯＫ
		;極扶她
		IF HAS_PENIS(TARGET) == 1
			;你もちんこついてる（男または扶她）
			IF HAS_PENIS(MASTER) == 1
				IF PREVCOM == 1;舐陰
					;極口交中
					IF TEQUIP:TARGET:口接触部位 == 1
						PRINTFORML 「ひゃうんっ！？　あっ、こらぁ、今舐めたら、駄目だってばぁ…」
						PRINTFORML 阴茎の根元から蟻の門渡りまで、舌で一気になぞると、極はびくりと尻を震わせた
						PRINTW 
					;極舐陰中
					ELSE
						PRINTFORML 目の前でパクパクと開閉する牝穴に誘われ、悄悄的口付けた
						PRINTFORML 「ふぁんっ%UNICODE(0x2665) *1%　稍微ぉ、今は私の番だってばぁ…%UNICODE(0x2665) *1%」
						PRINTW 
