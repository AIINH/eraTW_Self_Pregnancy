;==================================================
; 子宮口関数
;==================================================
@M_KOJO_MESSAGE_SIKYUKO_K9()
;ポルチオ性感持ち(EXP:57 > 50)か,綻び状態(EXP:57 > 25)か、それ以外か
IF TALENT:78 == 1
	SELECTCASE RAND:4
		CASE 0
			CALL HPH_PRINT,@"「啊啊、那里、那里HPH　喜欢那裏被玩弄HPH」","W"
		CASE 1
			CALL HPH_PRINT,@"「子宮的、入口、再多一点、多一点HPH」","W"
		CASE 2
			CALL HPH_PRINT,@"「啊唔HPH　那里、我的子宫口被撞击着HPH　好开心啊HPH」","W"
		CASE 3
			CALL HPH_PRINT,@"「啊呼HPH　要变成笨蛋了HPH、子宫口、紧紧、缠上了HPH」","W"
	ENDSELECT
ELSEIF EXP:57 < 50 && EXP:57 > 25
	CALL HPH_PRINT,@"「啊哈、啊啊！？　骗人、呀、总、总觉得、很、舒服HPH」","W"
	CALL HPH_PRINT,@"「不行不行、再这样的话……会、变成、笨蛋的HPH」","W"
ELSE
	PRINTFORML 「咕……啊……那、那地方……啊、不疼……了……」
ENDIF


