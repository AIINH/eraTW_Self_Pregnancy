;==================================================
;6,挿入Ｇスポ責め
;==================================================
@M_KOJO_MESSAGE_SCOM_K9_6
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K9_6_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K9_6_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORML 「……怎么了？结束了……吗？」
		PRINTFORMDW %CALLNAME:MASTER%的动作停止了、梦美很惊讶。
		PRINTFORMDW 当然这还不算完、%CALLNAME:MASTER%稍微改变了一下腰部的角度、
		PRINTFORMDW 瞄准梦美膣内的"那里"再次开始抽送。
		CALL HPH_PRINT,@"「唔ーーー～～～HPH！？　啊、别这样、太厉害了……HPH！？」","W"
		PRINTFORMDW 梦美第一次感受到这种快感、身体剧烈颤抖。
		RETURN 1
	ENDIF

	;基本セット
	IF LOCAL:1 && TCVAR:100 == PLAYER
		;前回射精した
		IF TCVAR:102
			;恋慕
			IF TALENT:恋慕
				CALL HPH_PRINT,@"「啊、咿呀、更多的涂抹上来吧HPH」","W"
				CALL HPH_PRINT,@"「我已经是、%CALLNAME:MASTER%的、女人了、深深的烙印在我身体里吧HPH」","W"
				RETURN 1
			;愛欲Orセフレ
			ELSEIF TALENT:愛欲 || TALENT:炮友
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「啊哈HPH　咿呀、那里、更多的涂抹上来吧HPH」","W"
						CALL HPH_PRINT,@"「这里、是%CALLNAME:MASTER%、你专用的洞HPH」","W"
					CASE 1
						CALL HPH_PRINT,@"「讨厌HPH　那种、地方不用做……标记HPH」","W"
						CALL HPH_PRINT,@"「我的、小穴、哈啊、哪里也、咿、不會去、的唷HPH」","W"
				ENDSELECT
				RETURN 1
			;それ以外
			ELSE
				CALL HPH_PRINT,@"「唔、%CALLNAME:MASTER%才刚射进来、啊HPH」","W"
				PRINTFORMDW 就像把精液涂进梦美的阴道壁一样、%CALLNAME:MASTER%改变着腰部的动作
				CALL HPH_PRINT,@"「唔啊、等……等、涂在、啊、那种……地方HPH」","W"
				RETURN 1
			ENDIF
		ELSE
			IF TALENT:3
				CALL HPH_PRINT,@"「唔呼、喜欢、好喜欢啊HPH　那里、喜欢被摩擦HPH」","W"
				CALL HPH_PRINT,@"「再多一点、%CALLNAME:MASTER%、再多一点HPH」","W"
				RETURN 1
			ELSEIF TALENT:184 || TALENT:185
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「啊哈HPH　咿呀、那里、再多一点HPH」","W"
						CALL HPH_PRINT,@"「这个、太棒了HPH　会、上瘾的吧HPH」","W"
					CASE 1
						CALL HPH_PRINT,@"「不行HPH　那里不行HPH　马上就会去的HPH」","W"
						PRINTFORMDW 是吗、%CALLNAME:MASTER%暂时停止了腰部动作、梦美立刻说出了真心话
						CALL HPH_PRINT,@"「开玩笑HPH　开玩笑的HPH　不要停下来啊HPH」","W"
				ENDSELECT
				RETURN 1
			ELSE
				PRINTFORML 「啊哈！那种、感觉、太刺激了！？\/\/\/」
				CALL HPH_PRINT,@"「别这样、那样的话、马、马上、就要、去、去了HPH」","W"
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
	
	;時姦中
	;IF FLAG:70
	;	PRINTFORMW 
	;	RETURN 1
	;恋慕
	;ELSEIF TALENT:恋慕
	;	PRINTFORMW 
	;	RETURN 1
	;屈服3
	;ELSEIF MARK:不埒刻印 == 3
	;	PRINTFORMW 
	;	RETURN 1
	;それ以外
	;ELSE
	;	PRINTFORMW 
	;	RETURN 1
	;ENDIF
ENDIF
