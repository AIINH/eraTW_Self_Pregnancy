					ELSE
						CALL HPH_PRINT,@"「不...!啊...HPH小弟弟...胀大了......!」","L"
						CALL HPH_PRINT,@"「%CALLNAME:MASTER%要去了吗...嗯...HPH好的...HPH尽管射...―」 ","L"
						CALL HPH_PRINT,@"「噢...噢...HPH啊...HPH HPH里面...HPH满满的...HPH HPH」","L"
						PRINTFORMDW %CALLNAME:MASTER%将阴茎插入清蘭的极限,然后射精...... 

					ENDIF
			ENDSELECT
