					PRINTFORMDL 清蘭被%CALLNAME:MASTER%舔花穴的快感,从阴茎前离开...... 

					CALL HPH_PRINT,@"「%CALLNAME:MASTER%...!等、等一下...HPH停下来...HPH」 ","L"
					PRINTFORMW (嗯...!我想让%CALLNAME:MASTER%舒服的......!) 

				CASE 1
					CALL HPH_PRINT,@"「嗯...啊啊...入口的地方...HPH 这样舔真是...啊啊...HPH」  ","L"
					CALL HPH_PRINT,@"「嗯...HPH啊...HPH嗯...HPH嗯...HPH嗯...HPH嗯...HPH啊...HPH」","L"
					PRINTFORMDW 清蘭一心一意地舔吮%CALLNAME:MASTER%的阴茎......! 

				CASE 2
					CALL SEIRAN_FELLATIO
			ENDSELECT
		ENDIF
	;それ以外
	ELSE
		;今射精した
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%...!啊...HPH嗯...HPH嗯...HPH等一下...HPH我快要到了...HPH」 ","L"
					CALL HPH_PRINT,@"「啊...!啊啊...HPH不行...HPH这样激烈的话...啊啊啊...HPH HPH」 ","L"
					PRINTFORMDW 清蘭发出娇声,身体抖个不停...... 

				CASE 1
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%...HPH 我...已经...!」 ","L"
					PRINTFORMDL %CALLNAME:MASTER%听见清蘭悲切的声音,唇贴在蜜穴,吸干了爱液。 

					CALL HPH_PRINT,@"「嗯嗯...啊啊啊...HPH HPH HPH」 ","L"
					CALL HPH_PRINT,@"「...哦...HPH啊...HPH这是什么...HPH头...嗡嗡的...HPH HPH」 ","W"
				CASE 2
					CALL SEIRAN_FELLATIO
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「看...!%CALLNAME:MASTER%也舒服...HPH」 ","L"
					PRINTFORMDL 清蘭将女阴压在%CALLNAME:MASTER%的脸上......  

					CALL HPH_PRINT,@"「啊...HPH嗯...HPH舌头,进来了...HPH」 ","W"
				CASE 1
					CALL HPH_PRINT,@"「啊...HPH嗯...HPH嗯...HPH」 ","L"
					CALL HPH_PRINT,@"(啊...HPH%CALLNAME:MASTER%这样努力地舔我的那里......HPH)","L"
