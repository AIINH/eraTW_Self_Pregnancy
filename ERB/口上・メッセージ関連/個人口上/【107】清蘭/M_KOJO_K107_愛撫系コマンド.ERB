				PRINTFORMDL 每用力揉捏一下,清蘭的身体就扭动,从阴道喷出爱液...

				CALL HPH_PRINT,@"「啊...啊...更多...更多...嗯...啊...」 ","L"
				PRINTFORMDW 看着无耻喘息的清蘭,%CALLNAME:MASTER%也再难抑制迷情......!

			CASE 2
				CALL HPH_PRINT,@"「嗯...啊...啊...果然...%CALLNAME:MASTER%摸我...很舒服...」 ","L"
				CALL HPH_PRINT,@"「啊...啊...%CALLNAME:MASTER%...更多摸我的阴蒂...求你...」 ","L"
				PRINTFORMDW 迎合清蘭的请求,细细爱抚她的阴蒂。 

		ENDSELECT
		RETURN 1
	;それ以外
	ELSE
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「嗯...啊...嗯...要出声了...!」 ","L"
				PRINTFORMDL 清蘭的阴蒂相当敏感。平时自己摸吗

				PRINTFORML 「自、自己!...我没那样过......」

				CALL HPH_PRINT,@"「唔...停下来!那里...不要转啊!」 ","L"
				CALL HPH_PRINT,@"「 有!我有过!」","W"
			CASE 1
				CALL HPH_PRINT,@"「嗯...啊...阴蒂...擦的话...不行...」","L"
				CALL HPH_PRINT,@"「啊啊啊!拉扯的话更不行!」","W"
			CASE 2
				CALL HPH_PRINT,@"「嗯...啊...呼...啊...嗯...」","L"
				PRINTFORMDL %CALLNAME:MASTER%爱抚清蘭的阴蒂,清蘭闭口喘气忍耐声音...

				PRINTFORMDL 为了让她发出声音,%CALLNAME:MASTER%用各种方式爱抚阴蒂,揉捏或拉扯。

				CALL HPH_PRINT,@"「嗯...呜...啊―...嗯!...嗯...嗯...」","L"
				PRINTFORMDW 清蘭似乎也固执,用手捂住嘴也要忍耐声音...... 

		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
RETURN 1

